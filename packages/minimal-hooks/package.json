{
  "name": "@minimals/hooks",
  "author": "Minimals",
  "version": "1.0.0",
  "description": "Some hooks used in Minimal UI and Zone UI",
  "keywords": [
    "typescript",
    "react",
    "hooks",
    "minimals hooks"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "clean": "rm -rf node_modules .turbo .next out dist build",
    "re:build": "pnpm clean && pnpm install && pnpm build",
    "ts:print": "npx tsc --showConfig"
  },
  "dependencies": {
    "@minimals/utils": "workspace:*",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "react": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.4.2",
    "@types/react": "18.3.4",
    "internal-eslint-config": "workspace:*",
    "internal-ts-config": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  }
}
