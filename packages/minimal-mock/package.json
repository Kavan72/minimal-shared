{
  "name": "@minimals/mock",
  "author": "Minimals",
  "version": "3.1.0",
  "description": "Vite Starter & TypeScript",
  "keywords": [
    "react",
    "hooks"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index",
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./src/utils/*/index.ts",
      "default": "./dist/utils/*/index.js"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "default": "./dist/hooks/index.js"
    },
    "./hooks/*": {
      "types": "./src/hooks/*/index.ts",
      "default": "./dist/hooks/*/index.js"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./src/components/*/index.ts",
      "default": "./dist/components/*/index.js"
    },
    "./test": {
      "types": "./src/test/index.ts",
      "default": "./dist/test/index.js"
    },
    "./test/*": {
      "types": "./src/test/*/index.ts",
      "default": "./dist/test/*/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-config-print.json",
    "clean": "rm -rf node_modules .turbo .next out dist build",
    "re:dev": "pnpm clean && pnpm install && pnpm dev",
    "re:build": "pnpm clean && pnpm install && pnpm build",
    "ts:print": "npx tsc --showConfig"
  },
  "dependencies": {
    "@mui/x-date-pickers": "^7.18.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "internal-eslint-config": "workspace:*",
    "internal-ts-config": "workspace:*",
    "@types/node": "^22.4.2",
    "@types/react": "18.3.4",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  }
}
