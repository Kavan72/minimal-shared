{
  "name": "@minimals/mock",
  "author": "Minimals",
  "version": "1.0.1",
  "description": "Simulation data is used for Minimal UI and Zone UI",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "keywords": [
    "mock data",
    "test data",
    "faker data",
    "simulation data"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./add": "./src/add/index.ts",
    "./subtract": "./src/subtract/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf node_modules .turbo .next out dist build"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "typescript": "latest",
    "@repo/typescript-config": "*"
  }
}
